<style>
  .box-container {
    margin-bottom: 4px;
  }
</style>

<div id="loading"></div>




<!-- MAIN SECTION -->


<div class="container">
  <h1 class="heading">CU Academics</h1>
  <div class="box-container">

    <% studyMaterial.forEach( (s) => { %>

      <div class="box">
        <div class="content">
          <h2><%= s.topic %></h2>
          <p><%= s.desc %></p>
          <a href="<%= s.link %>" class="btn">View</a>
          <div class="icons">
            <span><i class="fas fa-calendar"></i>Last Updated: 20th August, 2022</span>
          </div>
        </div>
      </div>
  
   <% } ) %>



  </div>
  <div id="load-more">Load More</div>
</div>





